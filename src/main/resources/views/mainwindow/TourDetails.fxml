<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="tourplanner.gui.controller.mainwindow.TourDetailsController"
            minWidth="300.0" prefHeight="160.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
    <HBox fx:id="hboxtourname" alignment="CENTER_RIGHT" prefHeight="42.0" prefWidth="438.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Label fx:id="tourtitle" prefHeight="25.0" prefWidth="312.0" text="Tourname" HBox.hgrow="NEVER">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Region prefHeight="42.0" prefWidth="23.0" HBox.hgrow="ALWAYS"/>
        <Button minWidth="40.0" mnemonicParsing="false" onAction="#updateTour" text="Edit" HBox.hgrow="NEVER"/>
        <Button mnemonicParsing="false" onAction="#deleteTour" text="Delete" HBox.hgrow="NEVER"/>
    </HBox>
    <GridPane fx:id="gridpanedata" layoutY="42.0" maxHeight="60.0" minHeight="60.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="von"/>
        <Label fx:id="start" text="Label" GridPane.columnIndex="1"/>
        <Label text="bis" GridPane.columnIndex="2"/>
        <Label fx:id="finish" text="Label" GridPane.columnIndex="3"/>
        <Label text="Distanz" GridPane.rowIndex="1"/>
        <Label fx:id="distance" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
    <SplitPane fx:id="splitpanetour" dividerPositions="0.5" layoutY="102.0" orientation="VERTICAL" prefHeight="200.0"
               prefWidth="160.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="102.0">
        <AnchorPane minHeight="160.0" minWidth="0.0" prefHeight="143.0" prefWidth="436.0">
            <TabPane layoutX="18.0" layoutY="-58.0" prefHeight="108.0" prefWidth="436.0"
                     tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                     AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <Tab text="Beschreibung">
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                        <Label fx:id="description" layoutX="14.0" layoutY="14.0"
                               text="Beschreibung"/>
                    </AnchorPane>
                </Tab>
                <Tab text="Bild">
                    <AnchorPane fx:id="imagepane" minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                prefWidth="200.0">
                        <ImageView fx:id="image" fitHeight="800.0" fitWidth="1200.0" layoutX="-14.0"
                                   layoutY="-37.0" pickOnBounds="true" preserveRatio="true"
                                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </AnchorPane>
                </Tab>
            </TabPane>
        </AnchorPane>
        <fx:include fx:id="logDetails" source="LogDetails.fxml"/>
